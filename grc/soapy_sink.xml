	#if $bw0()
self.$(id).set_bandwidth(0, $bw0)
	#end if
	#if $dc_offset_auto_mode0()
self.$(id).set_dc_offset(0, $dc_offset_auto_mode0)
	#end if
	#if $dc_offset0()
self.$(id).set_dc_offset (0, $dc_offset0, $dc_offset_auto_mode0)
	#end if
	#if $balance0()
self.$(id).set_iq_balance (0, $balance0)
	#end if
	#if $correction0()
self.$(id).set_frequency_correction (0, $correction0)
	#end if
#end if
